{
    "name": "tecnocampos/dynamic-model-filter",
    "description": "Apply request-based filters to Eloquent models in Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
          "name": "TecnoCampos",
          "email": "ti@tecnocampos.com.br"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "TecnoCampos\\DynamicModelFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TecnoCampos\\DynamicModelFilter\\RequestFiltersServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}